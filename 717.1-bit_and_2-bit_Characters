class Solution(object):
    def isOneBitCharacter(self, bits):
        """
        :type bits: List[int]
        :rtype: bool
        """
        lens=len(bits)
        i=0
        while i<lens:
            if i==lens-1:
                return True
            if bits[i]==1:
                i+=2
            else:
                i+=1
        return False
            
                
