class Solution(object):
    def removeElement(self, nums, val):
        """
        :type nums: List[int]
        :type val: int
        :rtype: int
        """
        res=0
        lens=len(nums)
        for i in range(lens):
            if nums[i]<>val:
                nums[res]=nums[i]
                res+=1
        return res
