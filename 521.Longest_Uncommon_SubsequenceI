class Solution(object):
    def findLUSlength(self, a, b):
        """
        :type a: str
        :type b: str
        :rtype: int
        """
        la=''.join(sorted(list(a)))
        lb=''.join(sorted(list(b)))
        if la==lb:
            return -1
        if len(la)>len(b):
            return len(la)
        else:
            return len(lb)
        
        
